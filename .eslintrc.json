{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "no-console": "warn",
    "prefer-const": "error",
    "indent": ["warn", 4],
    "max-len": ["error", {"code": 120}],
    "comma-dangle": ["error", "always-multiline"],
    "semi": "error"

  },
  "ignorePatterns": ["src/**/*.test.ts"]
}